<% if user_signed_in? %>
    <h1>Bienvenido <%= current_user.email %> </h1>
    <p><%= link_to "Cerrar sesion", "/logout", method: :delete %></p>
<% else %>
    <h1>Bienvenido</h1>
    <p>
        <%= link_to "Haz click aqui", "/auth/google_oauth2" %> para iniciar sesion con tu cuenta de google
    </p>
<% end %>
